@import '_global.sss'

ul
  list-style: none
  margin: 30px 0
  padding: 0
  display: flex

  & li
    display: flex
    jusity-content: center
    flex-direction: column
    margin-right: 15px
    align-items: center

    &:last-child
      margin-right: 0

    & img
      width: 100px
      height: 100px
      display: block
      margin-bottom: 7px
